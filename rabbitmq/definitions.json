  "queues": [
    { "name": "ph.control.generator", "vhost": "/", "durable": true, "auto_delete": false, "arguments": {} },
    { "name": "ph.control.moderator", "vhost": "/", "durable": true, "auto_delete": false, "arguments": {} },
    { "name": "ph.control.processor", "vhost": "/", "durable": true, "auto_delete": false, "arguments": {} },
    { "name": "ph.control.postprocessor", "vhost": "/", "durable": true, "auto_delete": false, "arguments": {} }
