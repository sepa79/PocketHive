  "bindings": [
    { "source": "ph.control", "vhost": "/", "destination": "ph.control.generator", "destination_type": "queue", "routing_key": "sig.#", "arguments": {} },
    { "source": "ph.control", "vhost": "/", "destination": "ph.control.moderator", "destination_type": "queue", "routing_key": "sig.#", "arguments": {} },
    { "source": "ph.control", "vhost": "/", "destination": "ph.control.processor", "destination_type": "queue", "routing_key": "sig.#", "arguments": {} },
    { "source": "ph.control", "vhost": "/", "destination": "ph.control.postprocessor", "destination_type": "queue", "routing_key": "sig.#", "arguments": {} }
  ]
